integer1 = int(input("enter the number"))
integer2 = int(input("enter the number"))
integer3 = int(input("enter the number"))

if integer1>=integer2 and integer1>=integer3 :
    print("the maximum number is integer1")
elif integer2>=integer1 and integer2>=integer3:
    print("the maximum no is integer2")
else:
    print("the maximum no is c")    
