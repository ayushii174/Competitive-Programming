percentage = float(input("enter the percentage:"))

if(percentage>= 90):
    print("grade is : A")
elif(percentage>= 80):
    print("grade is : B")
elif(percentage>= 70):
    print("grade is : C")
elif(percentage>= 60):
    print("grade is : D")
else:
    print("grade is : F")
